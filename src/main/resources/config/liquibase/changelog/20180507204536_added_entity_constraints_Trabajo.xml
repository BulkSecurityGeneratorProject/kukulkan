<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Trabajo.
    -->
    <changeSet id="20180507204536-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="empleado_id"
                                 baseTableName="trabajo"
                                 constraintName="fk_trabajo_empleado_id"
                                 referencedColumnNames="id"
                                 referencedTableName="empleado"/>

        <addForeignKeyConstraint baseColumnNames="trabajos_id"
                                 baseTableName="trabajo_tarea"
                                 constraintName="fk_trabajo_tarea_trabajos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="trabajo"/>
        <addForeignKeyConstraint baseColumnNames="tareas_id"
                                 baseTableName="trabajo_tarea"
                                 constraintName="fk_trabajo_tarea_tareas_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tarea"/>
        
    </changeSet>
</databaseChangeLog>
